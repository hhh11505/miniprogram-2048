<!--pages/rule/index.wxml-->

<swiper class="activity" indicator-dots="{{indicatorDots}}"
            autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
      <block wx:for="{{imgUrls}}">
        <swiper-item>
          <image src="{{item}}" class="slide-image" width="500" height="162"/>
        </swiper-item>
      </block>
    </swiper>

    <image src='/img/rule2.jpg' mode="widthFix"></image>
  
